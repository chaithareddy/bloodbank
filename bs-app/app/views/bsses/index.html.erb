<%- model_class = Bss -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:Name_of_BB) %></th>
      <th><%= model_class.human_attribute_name(:blood_typr) %></th>
      <th><%= model_class.human_attribute_name(:Quantity) %></th>
      <th><%= model_class.human_attribute_name(:Address) %></th>
      <th><%= model_class.human_attribute_name(:Contact_number) %></th>
      <th><%= model_class.human_attribute_name(:email_id) %></th>
      <th><%= model_class.human_attribute_name(:Working_hours) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @bsses.each do |bss| %>
      <tr>
        <td><%= link_to bss.id, bss_path(bss) %></td>
        <td><%= bss.Name_of_BB %></td>
        <td><%= bss.blood_typr %></td>
        <td><%= bss.Quantity %></td>
        <td><%= bss.Address %></td>
        <td><%= bss.Contact_number %></td>
        <td><%= bss.email_id %></td>
        <td><%= bss.Working_hours %></td>
        <td><%=l bss.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_bss_path(bss), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      bss_path(bss),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_bss_path,
            :class => 'btn btn-primary' %>
